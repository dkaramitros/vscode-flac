{
  "name": "FLAC / FISH",
  "scopeName": "source.flac",
  "patterns": [
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#numbers" },
    { "include": "#punctuation" },
    { "include": "#operators" },
    { "include": "#fish-function-block" },
    { "include": "#primary-commands" },
    { "include": "#fish-primary" },
    { "include": "#range-modifier-keywords" },
    { "include": "#config-solver-options" },
    { "include": "#variables-mech" },
    { "include": "#variables-flowthermal" },
    { "include": "#models" },
    { "include": "#properties" },
    { "include": "#structure-keywords" },
    { "include": "#io-alias-keywords" },
    { "include": "#fish-intrinsics" },
    { "include": "#fish-data-access" },
    { "include": "#fish-io" }
  ],
  "repository": {
    "comments": {
      "name": "comment.line.semicolon",
      "match": ";.*$"
    },
    "strings": {
      "name": "string.quoted.double",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        { "name": "constant.character.escape", "match": "\\\\." }
      ]
    },
    "numbers": {
      "name": "constant.numeric",
      "match": "(?i)\\b(?:(?:0|[1-9]\\d*)(?:\\.\\d+)?(?:[eE][+-]?\\d+)?)\\b"
    },
    "operators": {
      "name": "keyword.operator",
      "match": "[+\\-*/=<>!&|^%]+"
    },
    "punctuation": {
      "patterns": [
        {
          "name": "punctuation.separator.comma",
          "match": ","
        },
        {
          "name": "keyword.operator.assignment",
          "match": "="
        }
      ]
    },

    "primary-commands": {
      "name": "entity.name.function",
      "match": "(?i)^(?=\\s*)(?:a(?:p(?:p(?:l(?:y)?)?)?)?|at(?:t(?:a(?:c(?:h)?)?)?)?|c(?:a(?:l(?:l)?)?)?|con(?:f(?:i(?:g)?)?)?|cont(?:i(?:n(?:u(?:e)?)?)?)?|co(?:p(?:y)?)?|cy(?:c(?:l(?:e)?)?)?|def(?:i(?:n(?:e)?)?)?|end|fix|fr(?:e(?:e)?)?|ge(?:n(?:e(?:r(?:a(?:t(?:e)?)?)?)?)?)?|giic|grid|gr(?:o(?:u(?:p)?)?)?|h(?:e(?:l(?:p)?)?)?|hi(?:s(?:t(?:o(?:r(?:y)?)?)?)?)?|ieb(?:o(?:u(?:n(?:d(?:a(?:r(?:y)?)?)?)?)?)?)?|i(?:n(?:i(?:t(?:i(?:a(?:l)?)?)?)?)?)?|int(?:e(?:r(?:f(?:a(?:c(?:e)?)?)?)?)?)?|interi(?:o(?:r)?)?|l(?:a(?:b(?:e(?:l)?)?)?)?|ma(?:r(?:k)?)?|m(?:o(?:d(?:e(?:l)?)?)?)?|mov(?:i(?:e)?)?|n(?:e(?:w)?)?|opt|pa(?:u(?:s(?:e)?)?)?|pl(?:o(?:t)?)?|pr(?:i(?:n(?:t)?)?)?|pro(?:p(?:e(?:r(?:t(?:y)?)?)?)?)?|q(?:u(?:i(?:t)?)?)?|re(?:s(?:t(?:o(?:r(?:e)?)?)?)?)?|ret(?:u(?:r(?:n)?)?)?|rez(?:o(?:n(?:e)?)?)?|sa(?:v(?:e)?)?|sc(?:l(?:i(?:n)?)?)?|se(?:t)?|so(?:l(?:v(?:e)?)?)?|ste(?:p)?|sto(?:p)?|str(?:u(?:c(?:t(?:u(?:r(?:e)?)?)?)?)?)?|sys(?:t(?:e(?:m)?)?)?|tab(?:l(?:e)?)?|ti(?:t(?:l(?:e)?)?)?|tr(?:a(?:c(?:k)?)?)?|unm(?:a(?:r(?:k)?)?)?|wa(?:t(?:e(?:r)?)?)?|w(?:i(?:n(?:d(?:o(?:w)?)?)?)?)?)\\b"
    },

    "range-modifier-keywords": {
      "name": "keyword.other",
      "match": "(?i)\\b(?:from|to|long|short|both|xy|aside|bside|line|segment|adjust|arc|bilinear|circle|column|row|surface|group|mark|region|notnull|nmregion|delete|remove|reset|range|var|rdev|add|multiply|begin|end|skip|vs|continue)\\b"
    },

    "config-solver-options": {
      "name": "keyword.other",
      "match": "(?i)\\b(?:ats|axisymmetry|cppudm|creep|dynamic|extra|gwflow|p_stress|thermal|tpflow|auto|clock|force|sratio|step|age|noage|elastic|geometry|update|synchronize|avisc|frac|freq|mass|stiffness|hysteretic|hyst|local|combined|rayleigh|dytime|dydt|dyn|crdt|creeptime|multistep|flow|gwdt|gwtime|implicit|thdt|thtime|fobl|fobu|lmul|umul|latency|fastflow|fastwb|funsat|aunb|munb|echo|message|log|output|autoname|overwrite|pcx|windows|postscript|dxf|emf|jpg|bmp|clipboard|grayscale|bw|color|legend|noscale|foreground|back|fos|associated|bracket|cycles|file|include|exclude|tension|jtension|structure|interface|resolution|perturb|wbulk)\\b"
    },

    "variables-mech": {
      "name": "variable.other",
      "match": "(?i)\\b(?:xdisp|ydisp|xacc|yacc|nacc|sacc|xforce|yforce|rforce|pressure|nstress|sstress|sxx|syy|szz|sxy|sig1|sig2|xtraction|ytraction|esxx|esyy|eszz|esxy|ssi|ssi3d|ssr|ssr3d|vsi|vsr|vsxx|vsyy|vszz|vsxy|angle|unbalance|state|asxx|bsxx|csxx|dsxx|asyy|bsyy|csyy|dsyy|aszz|bszz|cszz|dszz|asxy|bsxy|csxy|dsxy|sdif)\\b"
    },

    "variables-flowthermal": {
      "name": "variable.other",
      "match": "(?i)\\b(?:pp|gpp|head|discharge|leakage|well|temperature|flux|source|convection|radiation|tcont|saturation|rsat|fmodulus|ftension|seepage|nwpp|nwdischarge|nwwell|vga|vgp0|vgpcnw|vgpcw|udcoe|xflow|yflow|xconduct|yconduct|thermal|gw|mech|flow|xaccel|yaccel)\\b"
    },

    "models": {
      "name": "entity.name.type",
      "match": "(?i)\\b(?:null|elastic|anisotropic|mohr\\-coul|drucker|ubiquitous|ss|subiquitous|dy|cam\\-clay|cysoil|chsoil|hoek\\-brown|mhoek|load|cvisc|cwipp|power|pwipp|viscous|wipp|th_isotropic|th_anisotropic|th_general|th_null|th_ac_isotropic|finn)\\b"
    },

    "properties": {
      "name": "support.type",
      "match": "(?i)\\b(?:bulk_mod|shear_mod|density|cohesion|friction|dilation|tension|young|poiss|nuyx|nuzx|xmod|ymod|kshear|qdil|qvol|jangle|jcohesion|jfriction|jdilation|jtension|ctable|ftable|dtable|cptable|ttable|lambda|kappa|mp1|mpc|mv0|mv1|bulk_ref|shear_ref|p_ref|p_ini|rf|dilf|fricf|fricv|bulk_current|shear_current|a_wipp|b_wipp|d_wipp|n_wipp|act_energy|gas_c|temp|e_dot_star|e_primary|e_plastic|et_plastic|ev_plastic|conductivity|spec_heat|thexp|f_rho|f_t0|lconduct|lspec_heat|con1|con2|n_cond|biot_c|permeability|porosity|per_table|por_table)\\b"
    },

    "structure-keywords": {
      "patterns": [
        {
          "name": "entity.name.class",
          "match": "(?i)\\b(?:beam|cable|liner|pile|rockbolt|strip|support)\\b"
        },
        {
          "name": "variable.other",
          "match": "(?i)\\b(?:xdis|ydis|xvel|yvel|rvel)\\b"
        },
        {
          "name": "keyword.other",
          "match": "(?i)\\b(?:fix|free|initial|load|pin|slave|unslave)\\b"
        },
        {
          "name": "support.type",
          "match": "(?i)\\b(?:area|density|e|i|pmom|radius|kbond|sbond|cs_ncoh|cs_nstiff|tfstrain)\\b"
        }
      ]
    },

    "io-alias-keywords": {
      "name": "keyword.other",
      "match": "(?i)\\b(?:help|line|arrow|plot|history|table|dump|limits|list|max|min|read|write|name|alias|fullpalette|size|both|cross|noheader|number|pen|overlay|sclin|title|movie|block|fill|interval|inverse|magnify|maximum|minimum|zero|color|windows|postscript|dxf|bmp|pcx|jpg|clipboard|aacceleration|aforce|agwater|athermal|avelocity|adynamic|xdisp|ydisp|xvel|yvel|svel|nvel|sys|dir|copy|del|ren|type|axial|moment1|moment2|shear|node|element|xaccel|yaccel|xdisplace|ydisplace|xvelocity|yvelocity)\\b"
    },

    "fish-primary": {
      "name": "keyword.control",
      "match": "(?i)\\b(?:DEFINE|END|IF|THEN|ELSE|ENDIF|END_IF|CASEOF|CASE|ENDCASE|END_CASE|LOOP|ENDLOOP|END_LOOP|WHILESTEPPING|WHILE_STEPPING|SECTION|ENDSECTION|END_SECTION|EXIT|EXIT_SECTION|COMMAND|ENDCOMMAND|END_COMMAND|ARRAY|INT|FLOAT|STRING|CONSTITUTIVE_MODEL|CONSTITUTIVEMODEL|FRIEND|f_prop|OPT)\\b"
    },

    "fish-function-block": {
      "name": "meta.function",
      "begin": "(?i)^(?=\\s*)(DEFINE)\\b",
      "beginCaptures": { "1": { "name": "storage.type.function" } },
      "end": "(?i)^(?=\\s*)(END)\\b",
      "endCaptures": { "1": { "name": "storage.type.function" } },
      "patterns": [
        { "include": "#comments" },
        { "include": "#strings" },
        { "include": "#numbers" },
        { "include": "#fish-primary" },
        { "include": "#fish-intrinsics" },
        { "include": "#fish-data-access" },
        { "include": "#operators" }
      ]
    },

    "fish-intrinsics": {
      "name": "support.function",
      "match": "(?i)\\b(?:abs|acos|asin|atan|atan2|cos|exp|ln|log|sin|sqrt|tan|max|min|sgn|float|int|string|fstring|type|and|or|not|in|out|parse|pre_parse|cparse|error|do_update|get_mem|lose_mem|rez_exe|mat_inverse|mat_transpose|sclose|sopen|sread|swrite)\\b"
    },

    "fish-data-access": {
      "patterns": [
        {
          "name": "variable.other",
          "match": "(?i)\\b(?:error|nerr|ierr|jerr|large|tenflg|s_3dd|s_dyn|s_echo|s_flow|s_imp|s_log|s_mech|s_mess|s_movie|step|cycle|crtime|dytime|gwtime|thtime|igp|jgp|izones|jzones|unbal|mech_ratio|unbflow|fos|fos_f|app_pnt|appgw_pnt|appth_pnt|att_pnt|ieb_pnt|int_pnt|str_pnt|tab_pnt|trac_pnt|udm_pnt|imem|fmem|cf_axi|cf_creep|cf_dyn|cf_ext|cf_gw|cf_ps|cf_therm|cm_max|sm_max|crtdel|dytdel|gwtdel|thtdel|xgrav|ygrav|wdens|wbulk|wbiot|v_ngw|v_nmech|v_ntherm|fc_arg)\\b"
        },
        {
          "name": "variable.other",
          "match": "(?i)\\b(?:x|y|xdisp|ydisp|xvel|yvel|xacc|yacc|xbody|ybody|xforce|yforce|gpp|nwgpp|sat|rsat|temp|tflow|gflow|g2flow|fmod|f2mod|ftens|vga|vgp0|vgpcnw|vgpcw|damp|flags|ex_n|gmsmul)\\b"
        },
        {
          "name": "variable.other",
          "match": "(?i)\\b(?:sxx|syy|szz|sxy|vsxx|vsyy|vszz|vsxy|ssi|ssr|ssi3d|ssr3d|vsi|vsr|fsr|fsi|ev_plastic|e_plastic|et_plastic|ev_tot|pp|vol_strain|k11|k12|k22|wk11|wk12|wk22|poro2|udcoe|visrat|xflow|yflow|xnwflow|ynwflow|model|state|dy_state|flags|inimodel|zsub|zmsmul|area|density|ex_n|zart|zxbar|z_group|z_model|z_prop|zde11|zde12|zde22|zde33|zdpp|zdrot|zporos|zs11|zs12|zs22|zs33|ztea|zteb|ztec|zted|ztsa|ztsb|ztsc|ztsd|zvisc)\\b"
        },
        {
          "name": "support.function.table",
          "match": "(?i)\\b(?:table|xtable|ytable|table_size)\\b"
        }
      ]
    },

    "fish-io": {
      "name": "support.function.io",
      "match": "(?i)\\b(?:open|close|read|write|IO_READ|IO_WRITE|IO_FISH|IO_ASCII)\\b"
    }
  },
  "injectionSelector": "L:source.flac",
  "fileTypes": ["flac","fis","dat","in","inp"],
  "uuid": "a8f9d4be-6e0c-4e37-bd1d-1f6b8aa4c6f6"
}
